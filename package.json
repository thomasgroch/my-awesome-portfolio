{
  "name": "my-awesome-portfolio",
  "version": "1.0.4",
  "description": "my awesome portfolio frontend.",
  "author": "Thomas Letsch Groch <thomas.groch@gmail.com>",
  "scripts": {
    "serve": "yarn build:front && npx http-server dist/ -c-1 -o -a 0.0.0.0",
    "build": "run-p build:**",
    "bootstrap": "node ./bootstrap-fauna-database.js",
    "build:back": "netlify-lambda build lambda/ --timeout 7",
    "build:front": "vuepress build portfolio --dest dist",
    "debug": "npx nodejs-dashboard -- yarn dev",
    "dev": "yarn start:front",
    "lint": "eslint \"./portfolio/.vuepress/**/*\" --ext .js,.vue --fix",
    "prebuild": "echo 'setup faunaDB'; yarn bootstrap",
    "precommit": "yarn lint",
    "start": "run-s start:**",
    "start:back": "netlify-lambda serve lambda/ --timeout 8 -c webpack.server.js",
    "start:front": "vuepress dev portfolio --host 0.0.0.0 --port 8080"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-brands-svg-icons": "5.14.0",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "@silvanite/vuepress-plugin-markdown-classes": "1.0.2",
    "@silvanite/vuepress-plugin-tailwind": "1.3.0",
    "@stackblitz/sdk": "1.5.0",
    "@vssue/api-github-v3": "1.4.4",
    "@vssue/vuepress-plugin-vssue": "1.4.6",
    "@vuepress/plugin-back-to-top": "1.5.2",
    "animate.css": "3.7.2",
    "axios": "0.21.1",
    "darkmode-js": "1.5.6",
    "dotenv-webpack": "1.7.0",
    "faunadb": "2.14.2",
    "mailgun-js": "0.22.0",
    "markdown-it-footnote": "3.0.2",
    "moment": "2.27.0",
    "netlify-lambda": "1.6.3",
    "npm-run-all": "4.1.5",
    "postmark": "2.5.4",
    "tailwindcss": "1.5.2",
    "tippy.js": "5.1.3",
    "vee-validate": "2.2.0",
    "vue-textarea-autosize": "1.1.1",
    "vue-typed-js": "0.1.2",
    "vuepress": "next",
    "vuepress-plugin-demo-code": "0.5.0",
    "vuepress-plugin-feed": "0.1.8",
    "vuepress-plugin-reading-time": "0.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.4.6",
    "babel-eslint": "^10.0.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-html": "6.0.2",
    "eslint-plugin-vue": "6.2.2",
    "gitmoji-cli": "3.2.6",
    "humans-generator": "2.1.1"
  },
  "homepage": "https://thomasgroch.com.br/",
  "keywords": [
    "blog",
    "developer",
    "markdown",
    "static",
    "vuejs",
    "vuepress"
  ],
  "license": "MIT",
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thomasgroch/my-awesome-portfolio"
  }
}
