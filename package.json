{
  "name": "my-awesome-portfolio",
  "version": "1.0.4",
  "description": "my awesome portfolio frontend.",
  "keywords": [
    "blog",
    "developer",
    "markdown",
    "static",
    "vuejs",
    "vuepress"
  ],
  "homepage": "https://thomasgroch.com.br/",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/thomas.groch/my-awesome-portfolio"
  },
  "license": "MIT",
  "author": "Thomas Letsch Groch <thomas.groch@gmail.com>",
  "scripts": {
    "start": "run-s start:**",
    "start:front": "vuepress dev portfolio --host 0.0.0.0 --port 8080",
    "start:back": "netlify-lambda serve lambda/ --timeout 8 -c webpack.server.js",
    "build": "run-p build:**",
    "build:front": "vuepress build portfolio --dest dist",
    "build:back": "netlify-lambda build lambda/ --timeout 7",
    "prebuild": "echo 'setup faunaDB'; yarn bootstrap",
    "dev": "yarn start:front",
    "debug": "npx nodejs-dashboard -- yarn dev",
    "lint": "eslint portfolio/**/ --ext .js,.vue --ignore-path .gitignore portfolio",
    "precommit": "yarn lint",
    "serve": "yarn build:front && npx http-server dist/ -c-1 -o -a 0.0.0.0",
    "bootstrap": "node ./bootstrap-fauna-database.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-brands-svg-icons": "5.12.0",
    "@fortawesome/free-regular-svg-icons": "5.12.0",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/vue-fontawesome": "0.1.7",
    "@silvanite/vuepress-plugin-markdown-classes": "1.0.2",
    "@silvanite/vuepress-plugin-tailwind": "1.2.0",
    "@stackblitz/sdk": "1.3.0",
    "@vssue/api-github-v3": "1.1.2",
    "@vssue/vuepress-plugin-vssue": "1.2.2",
    "@vuepress/plugin-back-to-top": "1.2.0",
    "animate.css": "3.7.2",
    "axios": "0.19.0",
    "darkmode-js": "1.5.3",
    "dotenv-webpack": "1.7.0",
    "faunadb": "2.10.0",
    "mailgun-js": "0.22.0",
    "markdown-it-footnote": "3.0.2",
    "moment": "2.24.0",
    "netlify-lambda": "1.6.3",
    "npm-run-all": "4.1.5",
    "postmark": "2.3.4",
    "tailwindcss": "1.1.4",
    "tippy.js": "5.1.2",
    "vee-validate": "2.2.0",
    "vue-textarea-autosize": "1.1.1",
    "vue-typed-js": "0.1.2",
    "vuepress": "next",
    "vuepress-plugin-demo-code": "0.4.2",
    "vuepress-plugin-feed": "0.1.8",
    "vuepress-plugin-reading-time": "0.1.1"
  },
  "devDependencies": {
    "dotenv": "8.2.0",
    "eslint": "6.7.2",
    "eslint-config-standard": "14.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-vue": "5.2.3",
    "gitmoji-cli": "2.2.0",
    "humans-generator": "2.1.1"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
